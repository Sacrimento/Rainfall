p(buf, str):
{
	buffer[4104];

	puts(str);
	read(0, buff, 4096);
	ptr = strchr(buff, "\n");
	return strncpy(argv[2], ptr, 20);
}

pp(char *dest):
{
	buf1[48];
	buf2[28];

	str1 = p(buf1, " - ");
	str2 = p(buf2, " - ");

	strcpy(dest, str1);

	return strcat(dest, str2);
}

