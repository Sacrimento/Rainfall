m:
t = time(0);
printf("%s - %d\n", t, str);

main:
str1 = malloc(8)
edx = malloc(8)
eax = str1
str2 = malloc(8)
edx = malloc(8)

eax = str2
eax+4 = edx
eax = ebp+0xc
eax += 4
edx = eax

strcpy(str1, edx);

eax = ebp+0xc
eax += 8
eax = [eax]
edx = eax

esp+0x4 = edx
strcpy(str2, edx);

file = fopen("/home/user/level8/.pass", "r");
