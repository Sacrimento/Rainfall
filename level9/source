N::setAnnotation(char*)
{
	char	buff[16]; 
	size_t	n;

	n = strlen(str);
	memcpy(buff, str, n);
}

N::N(int value)
{
	edx = value;
}

main(int arc, char **argv)
{
	class N *elem1;
	class N *elem2;

	if (argc < 2)
		exit (1);

	elem1 = new N(108);
	elem1.N(5);
	elem2 = new N(108);
	elem2.N(6);

	result = elem1.setAnnotation(argv[1]);

	func = *result;

	func(elem1, elem2);
}
